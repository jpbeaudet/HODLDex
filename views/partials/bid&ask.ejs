                <div class="col-lg-12">
                <h4 class="title boldy mb-5 mt-15">HODL Bid & Ask </h4>          
                <div class="col-lg-6">
                    <section class="box">
                        <header class="panel_header">
                            <h2 class="title pull-left">Current Asks (buy HODL) </h2>
                            <div class="actions panel_actions pull-right">
                                <a class="box_toggle fa fa-chevron-down"></a>
                            </div>
                        </header>
                        <div class="content-body">
                            <div class="row">
                                <div class="col-xs-12">

                                    <div class="table-responsive" data-pattern="priority-columns">
                                        <table id="tech-companies-1" class="table vm table-small-font no-mb table-bordered table-striped">
                                            <thead>
                                                <tr>
                                                    <th>Address <br><small> Date/Time </small></th>
                                                    <th></th>
                                                    <th>Amount <small> HODL </small></th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                            <%if (public.getAsks.amount.length > 0) { %>
                                            <% for(var i=0; i < public.getAsks.amount.length ; i++) {%>
                                                <tr>
                                                    <td>                                                        
                                                        <a href="address/<%= public.getAsks.address[i]%>"> <%= public.getAsks.address[i]%></a>
                                                    </td>
                                                    <td></td>
                                                    <td class="green-text boldy"><%= public.getAsks.amount[i]%></td>
                                                </tr>
                                              <% } %>
                                               <% } else{ %>  
                                                <tr>
                                                    <td>                                                        
                                                        <p> No Asks available at the moment </p>
                                                    </td>
                                                    <td></td>
                                                    <td class="green-text boldy"></td>
                                                </tr>
												<% } %>
                                            </tbody>
                                        </table>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </section>
                    
                </div>

